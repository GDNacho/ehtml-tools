{
    "EHTML e-html": {
        "prefix": "e_html",
        "body": [
            "<e-html data-src=\"${1}\"></e-html>"
        ]
    },
    "EHTML e-json": {
        "prefix": "e_json",
        "body": [
            "<e-json",
            "\tdata-src=\"${1}\"",
            "\tdata-response-name=\"${2}\"",
            "\tdata-actions-on-response=\"${3}\">",
            "\t${0}",
            "</e-json>"
        ]
    },
    "EHTML e-form": {
        "prefix": "e_form",
        "body": [
            "<e-form>",
            "\t<button",
            "\t\tdata-request-url=\"${1}\"",
            "\t\tdata-request-method=\"${2}\"",
            "\t\tdata-request-headers=\"${3}\"",
            "\t\tdata-response-name=\"${4}\"",
            "\t\tonclick=\"this.form.submit(this)\"",
            "\t\tdata-actions-on-response=\"${5}\"",
            "\t></button>",
            "</e-form>"
        ]
    },
    "EHTML e-form-dynamic-value": {
        "prefix": "e_fromdv",
        "body": [
            "<e-form-dynamic-value name=\"${1}\" data-bound-to=\"${2}\"></e-form-dynamic-value>"
        ]
    },
    "EHTML e-local-storage-value": {
        "prefix": "e_localv",
        "body": [
            "<e-local-storage-value name=\"${1}\" data-key=\"${2}\"></e-local-storage-value>"
        ]
    },
    "EHTML e-local-session-value": {
        "prefix": "e_sessionv",
        "body": [
            "<e-local-session-value name=\"${1}\" data-key=\"${2}\"></e-local-session-value>"
        ]
    },
    "EHTML e-github-oauth-button": {
        "prefix": "e_github",
        "body": [
            "<e-github-oauth-button",
            "\tdata-client-id=\"${1}\"",
            "\tdata-redirect-uri=\"${2}\"",
            "\tdata-scope=\"${3}\">",
            "\t${0}",
            "</e-github-oauth-button>"
        ]
    },
    "EHTML e-select": {
        "prefix": "e_select",
        "body": [
            "<e-select name=\"${1}\" value=\"${2}\">${0}</e-select>"
        ]
    },
    "EHTML e-svg": {
        "prefix": "e_svg",
        "body": [
            "<e-svg data-src=\"${1}\"></e-svg>"
        ]
    },
    "EHTML e-markdown": {
        "prefix": "e_md",
        "body": [
            "<e-markdown data-src=\"${1}\"></e-svg>"
        ]
    },
    "EHTML e-json-view": {
        "prefix": "e_jsonv",
        "body": [
            "<e-json-view data-src=\"${1}\"></e-json-view>"
        ]
    },
    "EHTML template e-json": {
        "prefix": "e_t_json",
        "body": [
            "<template is=\"e-json\" data-src=\"${1}\" data-object-name=\"${2}\">${0}</template>"
        ]
    },
    "EHTML template e-for-each": {
        "prefix": "e_t_foreach",
        "body": [
            "<template is=\"e-for-each\" data-list-to-iterate=\"${1}\" data-item-name=\"${2}\">${0}</template>"
        ]
    },
    "EHTML template e-if": {
        "prefix": "e_t_if",
        "body": [
            "<template is=\"e-if\" data-condition-to-display=\"${1}\">${0}</template>"
        ]
    },
    "EHTML template e-reusable": {
        "prefix": "e_t_reuseable",
        "body": [
            "<template is=\"e-reusable\" id=\"${1}\" data-object-name=\"${2}\">${0}</template>"
        ]
    },
    "EHTML template e-page-with-url": {
        "prefix": "e_t_pageurlparams",
        "body": [
            "<template is=\"e-page-with-url\" data-url-pattern=\"${1}\">${0}</template>"
        ]
    },
    "EHTML template e-wrapper": {
        "prefix": "e_t_wrapper",
        "body": [
            "<template",
            "\tis=\"e-wrapper\"",
            "\tdata-src=\"${1}\"",
            "\tdata-where-to-place=\"${2}\"",
            "\tdata-how-to-place=\"${3}\">",
            "\t${0}",
            "</template>"
        ]
    },
    "EHTML template e-ws": {
        "prefix": "e_t_ws",
        "body": [
            "<template",
            "\tis=\"e-ws\"",
            "\tdata-src=\"${1}\"",
            "\tdata-socket-name=\"${2}\"",
            "\tdata-actions-on-open-connection=\"${3}\">",
            "\t${0}",
            "</template>"
        ]
    },
    "EHTML:WS template e-json": {
        "prefix": "e_t_wsjson",
        "body": [
            "<e-json",
            "\tdata-socket=\"${1}\"",
            "\tdata-response-name=\"${2}\"",
            "\tdata-actions-on-response=\"${3}\">",
            "\t${0}",
            "</e-json>"
        ]
    },
    "EHTML:WS template e-form": {
        "prefix": "e_t_wsform",
        "body": [
            "<e-form>",
            "\t<button",
            "\t\tdata-socket=\"${1}\"",
            "\t\tonclick=\"this.form.submit(this)\"",
            "\t></button>",
            "</e-form>"
        ]
    }
}